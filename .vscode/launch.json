    "version": "0.2.0",
    "configurations": [
        {
            "name": "TMDB (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main_prod.dart",
            "args": [
                "--flavor",
                "prod",
                "--dart-define-from-file",
                "config/prod/config.json"
            ]
        },
        {
            "name": "TMDB (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main_prod.dart",
            "args": [
                "--flavor",
                "prod",
                "--dart-define-from-file",
                "config/prod/config.json"
            ]
        },
        {
            "name": "TMDB (dev)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/main_dev.dart",
            "args": [
                "--flavor",
                "dev",
                "--dart-define-from-file",
                "config/dev/config.json"
            ]
        }
    ]
}